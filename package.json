{
  "name": "react-ssr-cnode",
  "version": "1.0.0",
  "description": "react cnode",
  "main": "index.js",
  "scripts": {
    "build:client": "webpack --config build/webpack.config.client.js",
    "build:server": "webpack --config build/webpack.config.server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.client.js",
    "dev:server": "cross-env NODE_ENV=development nodemon server/server.js",
    "clear": "rimraf dist",
    "build": "npm run clear && npm run build:client && npm run build:server",
    "lint": "eslint --ext .js --ext .jsx client/",
    "precommit": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/happystory/react-ssr-cnode.git"
  },
  "keywords": [
    "cnode"
  ],
  "author": "liuyao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/happystory/react-ssr-cnode/issues"
  },
  "homepage": "https://github.com/happystory/react-ssr-cnode#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "koa": "^2.5.1",
    "koa-mount": "^3.0.0",
    "koa-proxies": "^0.7.0",
    "koa-static": "^4.0.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "memory-fs": "^0.4.1",
    "nodemon": "^1.17.4",
    "rimraf": "^2.6.2",
    "webpack": "^4.7.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  }
}
